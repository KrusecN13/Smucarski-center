    function Delete_Cookie( name, path, domain ) {
        if (document.cookie.indexOf(name+'=') >= 0) document.cookie = name + "=" +
        ( ( path ) ? ";path=" + path : "") +
        ( ( domain ) ? ";domain=" + domain : "" ) +
        ";expires=Thu, 01-Jan-1970 00:00:01 GMT";
    }
    
        //if( /cellphone|mobile|wap|wml|Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
    var forcedesktop = false;
    if(window.location.hash && window.location.hash == '#forcedesktop'){
        forcedesktop = true;
        Delete_Cookie('inapp','/','fis-ski.com');
        var today = new Date();
        var expiresDate = new Date();
        expiresDate.setDate(today.getDate()+365);
        expiresDate.toUTCString();
        document.cookie = 'fisskimobiletodesktop=mobiletodesktop;expires='+expiresDate+';path=/;domain=fis-ski.com';
    }
    if (window.document.location.href.indexOf('/slope-survey/') != -1){
        forcedesktop = true;
    }
    if (document.cookie.indexOf('inapp=') >= 0 ){
        if (window.document.location.href.indexOf('dynamic/athlete-biography.html') != -1){
            window.document.location = 'https://mobile.fis-ski.com/mobile/mobile-biographies.html'+window.document.location.search+'&type=result';
        } else {
            window.document.location = 'https://mobile.fis-ski.com?from=data';
        }
    }
    if (window.outerWidth > 0 && window.outerWidth < 768 && forcedesktop == false && document.cookie.indexOf('fisskimobiletodesktop=') == -1){
        var gotomobile = confirm("Your screen size is more adapted for the FIS website mobile version.\nDo you want to switch to the mobile version?");
        if (gotomobile){
            window.document.location = 'https://mobile.fis-ski.com?from=data';
        } else {
            Delete_Cookie('inapp','/','fis-ski.com');
            var today = new Date();
